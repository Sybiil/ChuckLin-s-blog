{
    "author": "Chuck Lin <mikumiku.lch@hotmail.com>",
    "description": "给您添麻烦了",
    "title": "给大家添麻烦了",
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "splitter",
        "expandable-chapters-small",
        "tbfed-pagefooter",
        "anchor-navigation-ex",
        "anchors",
        "disqus"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy chuck Lin 2017",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex": {
                "showLevel": false,
                "associatedWithSummary": true,
                "mode": "float",
                "float": {
                    "showLevelIcon": false,
                    "level1Icon": "fa fa-hand-o-right",
                    "level2Icon": "fa fa-hand-o-right",
                    "level3Icon": "fa fa-hand-o-right"
                },
                "pageTop": {
                    "showLevelIcon": false,
                    "level1Icon": "fa fa-hand-o-right",
                    "level2Icon": "fa fa-hand-o-right",
                    "level3Icon": "fa fa-hand-o-right"
                }
        },
        "disqus": {
            "shortName": "gitbookuse"
        }
    }
}